<launch>
  <arg name="map_file" default="$(find carrier_ros_map_export)/maps/map.yaml"/>
  <arg name="open_rviz" default="true"/>
  <arg name="move_forward_only" default="false"/>

      <!-- <node pkg="tf" type="static_transform_publisher" name="map_to_odom"
      args="100.0 100.0 0.0 0.0 0.0 0.0 /map /odom 40" /> -->

  <!-- Map server -->
  <node pkg="carrier_ros_map_export" name="map_server" type="map_server" args="$(arg map_file)" output="screen"/>
  <node pkg="carrier_ros_map_export" name="map_export" type="map_export" output="screen" />

</launch>
